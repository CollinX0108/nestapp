{
  "version": 2,
  "framework": "nestjs",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "installCommand": "npm install"
}